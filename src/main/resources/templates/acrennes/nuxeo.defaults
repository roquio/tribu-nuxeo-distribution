#(acrennes-authentification-config.xml.nxftl)
nuxeo.auth.secret.key=none
nuxeo.auth.max.age=3600
nuxeo.auth.cas.appURL=http://localhost/nuxeo/nxstartup.faces
nuxeo.auth.cas.serviceLoginURL=https://localhost/casshib/shib/toutatice/login
nuxeo.auth.cas.serviceValidateURL=https://localhost/casshib/shib/666666/serviceValidate
nuxeo.auth.cas.proxyValidateURL=https://localhost/casshib/shib/666666/proxyValidate
nuxeo.auth.cas.serviceKey=service
nuxeo.auth.cas.logoutURL=https://localhost/casshib/shib/toutatice/logout

#(acrennes-pictures-config.xml.nxftl)
nuxeo.imagemagick.IMConvert=/usr/bin/convert
nuxeo.imagemagick.IMIdentify=/usr/bin/identify
nuxeo.imagemagick.IMStream=/usr/bin/stream

#(components.xml)
nuxeo.seam.conversationTimeout=1800000
nuxeo.seam.concurrentRequestTimeout=30000
nuxeo.seam.createTempFiles=true
nuxeo.seam.max.request.size=0

#(server.xml.nxftl)
nuxeo.server.http.automation.maxHttpHeaderSize=8192
nuxeo.server.http.automation.maxThreads=300
nuxeo.server.http.automation.acceptCount=100
nuxeo.server.http.automation.connectionTimeout=40000
nuxeo.server.http.automation.URIEncoding=UTF-8
nuxeo.server.http.automation.maxKeepAliveRequests=1
nuxeo.server.http.automation.disableUploadTimeout=false

#(server.xml.nxftl)
nuxeo.server.http.jsf.port=8181
nuxeo.server.http.jsf.maxHttpHeaderSize=8192
nuxeo.server.http.jsf.maxThreads=200
nuxeo.server.http.jsf.acceptCount=100
nuxeo.server.http.jsf.connectionTimeout=150000
nuxeo.server.http.jsf.disableUploadTimeout=false
nuxeo.server.http.jsf.URIEncoding=UTF-8
nuxeo.server.http.jsf.connectionUploadTimeout=3600000
nuxeo.server.http.jsf.proxyName=bloc-notes.ac-rennes.fr
nuxeo.server.http.jsf.proxyPort=80

#(server.xml.nxftl) custom params
acrennes.server.tomcat.enableJmxRemoteLifecycleListener=false

# (config/acrennes-authentification-config.xml.nxftl)
acrennes.nuxeo.auth.cas.enableTokenAuth=false

# (config/acrennes-ldap-cache-config.xml.nxftl)
nuxeo.ldap.cache.user.maxSize=10000
nuxeo.ldap.cache.user.withoutRef.maxSize=10000
nuxeo.ldap.cache.group.maxSize=10000
nuxeo.ldap.cache.group.withoutRef.maxSize=10000

